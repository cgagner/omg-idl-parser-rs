  "readonly" "attribute" type_spec readonly_attr_declarator
}

// (90)
readonly_attr_declarator: () = {
  simple_declarator raises_expr,
  simple_declarators
}

simple_declarators: () = {
  simple_declarator,
  simple_declarators "," simple_declarator,
}

// (91)
attr_spec: () = {
