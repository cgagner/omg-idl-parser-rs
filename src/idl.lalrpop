// (45)
struct_dcl: () = {
  struct_def,
  struct_forward_dcl,
}

// (46)
struct_def: () = {
  "struct" identifier "{" member+ "}",
  // (195) Building Block Extended Data-Types
  "struct" identifier ":" scoped_name "{" member* "}",
  "struct" identifier "{" "}",
}

// (47)
member: () = {
  type_spec declarators ";"
}

// (48)
struct_forward_dcl: () = {
  "struct" identifier,
}

// (49)
union_dcl: () = {
  union_def,
